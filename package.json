{
  "name": "freyja-react",
  "version": "0.1.9",
  "description": "UI component library built using the React framework",
  "main": "dist/freyja.bundle.js",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "polished": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-styleguidist": "^9.0.8",
    "serve": "^11.0.0",
    "styled-components": "^4.2.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "peerDependencies": {
    "polished": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "serve": "./node_modules/.bin/serve -s dist",
    "serve-prod": "./node_modules/.bin/serve -s showcase",
    "build": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "build-freyja": "rm -rf ./dist && webpack --mode production --config webpack.lib.config.js",
    "styleguidist": "npx styleguidist build && npx styleguidist server",
    "cleanup": "rm -rf ./dist && rm -rf ./showcase && rm -rf ./node_modules"
  },
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "keywords": [
    "Freyja",
    "React",
    "Midgard",
    "UI",
    "Library"
  ],
  "author": "Andrew Shortall",
  "license": "ISC"
}
